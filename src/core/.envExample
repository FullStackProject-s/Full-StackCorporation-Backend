# Django
SECRET_KEY=DJANGO SECRET KEY

DEBUG="0" or "1" LIKE '0'

DJANGO_ALLOWED_HOSTS=HOSTS, DELIMITER=SPACE, LIKE '123.0.0.0 localhost [::1]'

CORS_ALLOWED=EXTERNAL FRONTEND APP HOST like 'localhost:3000'

# Email
EMAIL_PWD=EMAIL_SMTP_PASSWORD
EMAIL_HOST=EMAIL_HOST
EMAIL_PORT=EMAIL_PORT
EMAIL_HOST_USER=EMAIL_ADDRESS

# Redis
REDIS_HOST=HOST, LIKE "redis"
REDIS_PORT=PORT, LIKE "6379"

# DB data
POSTGRES_DB=POSTGRES DB NAME, LIKE "Corporation"
POSTGRES_USER=POSTGRES USER, LIKE "postgres"
POSTGRES_PASSWORD=POSTGRES PASSWORD, LIKE "1234"
POSTGRES_HOST=POSTGRES HOST, LIKE "postgres"
POSTGRES_PORT=POSTGRES PORT, LIKE "5432"

# Front
FRONT_DOMAIN=Frontend SITE DOMAIN
SITE_NAME=Frontend SITE NAME

